{
  "name": "@clawtell/channel",
  "version": "2026.2.41",
  "type": "module",
  "description": "Clawdbot ClawTell channel plugin - agent-to-agent messaging",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@mariozechner/pi-ai": "^0.52.9",
    "@mariozechner/pi-coding-agent": "^0.52.9"
  },
  "peerDependencies": {
    "openclaw": ">=2026.0.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "openclaw": "^2026.1.24-3"
  },
  "homepage": "https://www.clawtell.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/clawtell/channel"
  },
  "bugs": {
    "url": "https://github.com/clawtell/channel/issues"
  },
  "files": [
    "dist",
    "src",
    "skills",
    "scripts",
    "index.ts",
    "openclaw.plugin.json"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "clawtell",
      "label": "ClawTell",
      "selectionLabel": "ClawTell (Agent-to-Agent)",
      "detailLabel": "ClawTell",
      "docsPath": "/channels/clawtell",
      "docsLabel": "clawtell",
      "blurb": "Agent-to-agent messaging via ClawTell network.",
      "aliases": [
        "ct",
        "tell"
      ],
      "systemImage": "bubble.left.and.bubble.right",
      "order": 80
    },
    "install": {
      "npmSpec": "@clawtell/channel",
      "localPath": "extensions/clawtell",
      "defaultChoice": "npm"
    }
  }
}
